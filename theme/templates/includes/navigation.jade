nav#J-head.site-nav
	a(href="/")
		img.logo(src="/static/uxcore.png")
	ul.nav-list
		each item in config.site.navigation
			li(data-id="#{item.id}")
				a(href="#{item.link}")= item.text
	div#J_ComponentsSearch.component-search
	- var pages = {};
	- for page, name in resource.pages
		- var newPage = JSON.parse(JSON.stringify(page));
		- delete newPage.meta.html;
		- pages[name] = newPage;
	div#J_ComponentsList.component-list #{JSON.stringify(pages)}
	img#J_ThemeSelector.theme-selector(src="/static/iconfont-yifuhuanfu.png")
	#J_ThemePickerBox.theme-picker-box
